{
  "size": 0,
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "@timestamp": {
              "gte": "now-<%= elk_time_range %>",
              "lte": "now"
            }
          }
        },
        {
          "term": {
            "user_access.jti": {
              "value": "<%= jti %>"
            }
          }
        }
      ]
    }
  },
      "aggs" : {
        "endpoints" : {
            "terms" : {
                "field" : "controller.keyword",
                "size" : 100
            },
            "aggs" : {
              "status-code" : {
                "terms": {
                  "field": "status"
                }
              }
            }
        }
    }
}
