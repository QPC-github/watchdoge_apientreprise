{
  "size": 10000,
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "@timestamp": {
              "gte": "now-12h",
              "lte": "now/d"
            }
          }
        },
        {
          "term": {
            "parameters.context.keyword": {
              "value": "Ping"
            }
          }
        }
      ],
      "must_not": [
        {
          "terms": {
            "controller.keyword": [
              "api/v1/ping",
              "api/v2/ping"
            ]
          }
        }
      ],
      "filter": {
        "exists": {
          "field": "user_access.resource"
        }
      }
    }
  },
  "sort": [
    {
      "@timestamp": {
        "order": "asc"
      }
    }
  ]
}
