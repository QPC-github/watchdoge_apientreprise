{
  "size": 50,
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "@timestamp": {
              "gte": "now-8d/d",
              "lte": "now/d"
            }
          }
        },
        {
          "term": {
            "user_access.jti.keyword": {
              "value": "<%= jti %>"
            }
          }
        }
      ]
    }
  },
  "sort": [
    {
      "@timestamp": {
        "order": "desc"
      }
    }
  ]
}
