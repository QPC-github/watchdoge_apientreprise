{
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "@timestamp": {
              "gte": "now-<%= elk_time_range %>",
              "lte": "now/d"
            }
          }
        },
        {
          "wildcard": {
            "source": "siade_production*"
          }
        }
      ],
      "must_not": [
        {
          "terms": {
            "controller.keyword": [
              "api/v1/ping",
              "api/v2/ping"
            ]
          }
        },
        {
          "term": {
            "parameters.context.keyword": {
              "value": "Ping"
            }
          }
        },
        {
          "terms": {
            "status": [
              "401",
              "403"
            ]
          }
        }
      ],
      "filter": {
        "exists": {
          "field": "controller"
        }
      }
    }
  }
}
