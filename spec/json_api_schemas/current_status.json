{
  "type": "object",
  "properties": {
    "results": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["name", "sub_name", "api_version", "code", "timestamp"],
        "properties": {
          "name": {
            "type": "string",
            "title": "The Name of the endpoint."
          },
          "sub_name": {
            "type": ["string", "null"],
            "title": "The sub_name of the endpoint can be null"
          },
          "api_version": {
            "type": "integer",
            "title": "The api_version of the endpoint",
            "minimun": 1,
            "maximum": 2
          },
          "code": {
            "type": "integer",
            "title": "The HTTP Code of the last Ping",
            "minimum": 200,
            "maximum": 550
          },
          "timestamp": {
            "type": "string",
            "title": "The Timestamp of the last Ping",
            "pattern": "^\\d{4}-\\d{2}-\\d{2}.\\d{2}:\\d{2}:\\d{2}"
          }
        }
      }
    }
  }
}
