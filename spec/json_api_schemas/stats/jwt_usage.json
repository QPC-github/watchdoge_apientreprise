{
  "type": "object",
  "additionalProperties": false,
  "required": ["last_calls", "number_of_calls", "http_code_percentages"],
  "properties": {
    "last_calls": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["uname", "name", "url", "params", "code", "timestamp"],
        "properties": {
          "uname": {
            "type": "string",
            "title": "The unique name of the http request"
          },
          "name": {
            "type": "string",
            "title": "The common name of the http request"
          },
          "url": {
            "type": "string",
            "title": "The Url of the http request"
          },
          "params": {
            "type": "array",
            "title": "The Params of the http request",
            "items": {
              "type": "object",
              "patternProperties": {
                "": {
                  "type": "string",
                  "title": "value of the param"
                }
              }
            }
          },
          "code": {
            "type": "integer",
            "title": "The HTTP Code of the http request",
            "minimum": 200,
            "maximum": 599
          },
          "timestamp": {
            "type": "string",
            "title": "the time of the call"
          }
        }
      }
    },
    "http_code_percentages": {
      "type": "object",
      "patternProperties": {
        "^last_[0-9]+_(minutes|hours|days)$": {
          "type": "array",
          "patternProperties": {
            "^[2-5][0-9]{2}$": {
              "type": "number",
              "title": "The percentage of 200 HTTP response",
              "minimum": 0,
              "maximum": 100
            }
          }
        }
      }
    },
    "number_of_calls": {
      "type": "object",
      "patternProperties": {
        "^last_[0-9]+_(minutes|hours|days)$": {
          "type": "object",
          "additionalProperties": false,
          "required": [
            "total",
            "by_endpoint"
          ],
          "properties": {
            "total": {
              "type": "integer",
              "title": "The Total number of calls during the period",
              "minimum": 0
            },
            "by_endpoint": {
              "type": "array",
              "items": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                  "uname",
                  "name",
                  "total"
                ],
                "properties": {
                  "uname": {
                    "type": "string",
                    "title": "The Uname of the endpoint"
                  },
                  "name": {
                    "type": "string",
                    "title": "The displayed name of the endpoint"
                  },
                  "total": {
                    "type": "integer",
                    "title": "The Total of call of the endpoint during the period",
                    "minimum": 0
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
