{
  "type": "object",
  "properties": {
    "number_of_calls": {
      "type": "object",
      "patternProperties": {
        "^last_[0-9]+_(minutes|hours|days)$": {
          "type": "object",
          "additionalProperties": false,
          "required": [
            "total",
            "by_endpoint"
          ],
          "properties": {
            "total": {
              "type": "integer",
              "title": "The Total number of calls during the period",
              "minimum": 0
            },
            "by_endpoint": {
              "type": "array",
              "items": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                  "uname",
                  "name",
                  "total"
                ],
                "properties": {
                  "uname": {
                    "type": "string",
                    "title": "The Uname of the endpoint"
                  },
                  "name": {
                    "type": "string",
                    "title": "The displayed name of the endpoint"
                  },
                  "total": {
                    "type": "integer",
                    "title": "The Total of call of the endpoint during the period",
                    "minimum": 0
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}